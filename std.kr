def |x: i32| mul(a: i32) {
    x * a
}

def |x: i32| as_i32() { x }

def alloc(size: i32) {
    let x: ptr = 0
    #c x = malloc(size);
    x:as_i32()
}

def drop(address: ptr) {
    #c free(address);
}

def print(x: i32) {
    #c printf("%d", x)
}

def println(x: i32) {
    #c printf("%d\n", x)
}

def array(size: i32) {
    alloc(size)
}

def |ary: ptr| at(idx: i32) {
    let el: i32 = 0
    #c el = ary[idx];
    el
}
