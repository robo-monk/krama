

def fib(a: i32) {
    if (a < 2) {
        1
    } else {
        :fib(a-1) + :fib(a-2)
    }
}

def fib2(a: i32) {
    if (a > 2) {
        :fib(a-1) + :fib(a-2)
    } else {
        1
    }
}

def as_float(a: f64) {
    a
}

def as_int(a: i32) {
    a
}


def main() {
    :fib2(39) + :fib(42)
}
